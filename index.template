<!doctype html>
<html lang="en">
<head>
    <title>CMS</title>
    <!-- styles -->
    <style is="custom-style">
        @font-face {
            font-family: GMsanC;
            src: url(assets/GMsanC_Reg.otf);
        }
    </style>
    <% _.each(styles, function(styleTag) { %>
        <%= styleTag %>
    <% }); %>

    <!-- imports -->
    <% _.each(imports, function(importTag) { %>
        <%= importTag %>
    <% }); %>
</head>
<body>
    <!-- widgets -->
    <% _.each(widgets, function(widget) { %>
        <div>
            <%= widget.html %>
        </div>
    <% }); %>

    <!-- scripts -->
    <% _.each(scripts, function(scriptTag) { %>
        <%= scriptTag %>
    <% }); %>
</body>
</html>